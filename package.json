{
  "name": "@interval/server",
  "description": "Interval Server is the central server for Interval apps",
  "version": "1.1.0-dev.0+internal",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.server.json -r tsconfig-paths/register prisma/seed.ts"
  },
  "bin": {
    "interval-server": "dist/src/entry.js"
  },
  "files": ["dist/"],
  "scripts": {
    "lint": "eslint src",
    "seed": "prisma db seed",
    "icons": "svgr src/icons/src/*.svg",
    "emails": "nodemon --config src/emails/nodemon.json",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "build": "rm -rf dist && prisma generate && yarn server:build && yarn wss:build && yarn client:build",
    "pkg": "./scripts/pkg.sh",
    "pub": "./scripts/pub.sh",
    "start": "NODE_ENV=production node ./dist/src/entry.js",
    "docker": "./scripts/docker-build.sh",
    "client:build": "vite build",
    "client:clean": "rm -r node_modules/.vite",
    "client:dev": "vite",
    "client:preview": "vite preview",
    "client:storybook": "storybook dev -p 6006 --ci",
    "client:storybook:build": "storybook build",
    "server:build": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "wss:build": "tsc --project tsconfig.wss.json  && tsc-alias -p tsconfig.wss.json",
    "test": "playwright test",
    "test:jest": "jest",
    "test:all": "ts-node --project tsconfig.server.json -r tsconfig-paths/register test/index.ts",
    "test:reconnect": "TEST_RECONNECT=1 playwright test reconnect",
    "test:uploads": "TEST_UPLOADS=1 playwright test -g upload",
    "test:dev": "nodemon --watch test --exec \"playwright test\" -e ts",
    "test:start": "NODE_ENV=test start-server-and-test start:proxy http://localhost:3000/health-check/app test",
    "test:start:all": "NODE_ENV=test start-server-and-test start:proxy http://localhost:3000/health-check/app test:all",
    "postinstall": "yarn prisma generate"
  },
